<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="go1"/>
            <SetBlackboard output_key="${valueA:s}" value="3"/>
            <Repeat num_cycles="${valueA:f}">
                <Action ID="go2"/>
            </Repeat>
            <Action ID="go3"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="go1"/>
        <Action ID="go2"/>
        <Action ID="go3"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

